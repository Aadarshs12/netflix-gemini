{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "react/prop-types": "off", // Disable prop-types if not used
    "no-unused-vars": "error", // Enforce unused variables
    "react-hooks/exhaustive-deps": "error" // Enforce hook dependencies
  }
}